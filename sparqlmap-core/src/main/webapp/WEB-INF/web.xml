<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>SparqlMap</display-name>
  <servlet>
    <servlet-name>sparql</servlet-name>
    <servlet-class>org.aksw.sparqlmap.SPARQLServlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>sparqlDummy</servlet-name>
    <servlet-class>org.aksw.sparqlmap.DummySparqlServlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>sparqlFindingDummy</servlet-name>
    <servlet-class>org.aksw.sparqlmap.DummyFindingServlet</servlet-class>
  </servlet>
      <servlet>
    <servlet-name>dump</servlet-name>
    <servlet-class>org.aksw.sparqlmap.BSBMDumpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sparql</servlet-name>
    <url-pattern>/sparql</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sparqlDummy</servlet-name>
    <url-pattern>/dummy</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>sparqlFindingDummy</servlet-name>
    <url-pattern>/finding</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
    <servlet-name>dump</servlet-name>
    <url-pattern>/dump</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>
